{
	"name": "@dulysse1/ts-branding",
	"version": "1.1.0",
	"description": "Decorate object properties and discover the power of Typescript brands âœ¨",
	"scripts": {
		"build": "tsup",
		"format:check": "prettier . --check",
		"format:fix": "prettier . --write",
		"lint": "npx eslint --fix .",
		"prepublishOnly": "pnpm build",
		"prepare": "husky",
		"check-exports": "attw --pack . --ignore-rules cjs-resolves-to-esm"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"keywords": [
		"node",
		"typescript",
		"type",
		"types",
		"safe",
		"tools",
		"typesafe"
	],
	"author": {
		"name": "Ulysse Dupont",
		"url": "https://github.com/dulysse",
		"email": "ulyssedupont2707@gmail.com"
	},
	"repository": {
		"url": "https://github.com/Dulysse/ts-branding",
		"type": "public"
	},
	"bugs": {
		"url": "https://github.com/Dulysse/ts-branding/issues"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"homepage": "https://github.com/Dulysse/ts-branding/blob/master/README.md",
	"private": false,
	"license": "ISC",
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.4",
		"@eslint/js": "^9.21.0",
		"eslint": "^9.21.0",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"prettier": "^3.5.2",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1"
	}
}
